{{#org-contact-section}}
<section class="contact">
  <div class="container">
    <div class="contact__main">
      <div class="contact__main__title text-center">
        {{> ../molecules/contact-title-h2}}
      </div>
      <div class="row">
        <div class="contact__main__item col-12 col-lg-4">
          <div class="contact__main__item__image" style="background-color:#00AE94;">
            {{> ../molecules/contact-item-img}}
          </div>
          <div class="contact__main__item__content">
            {{> ../molecules/contact-info-item}}
          </div>
        </div>

        <div class="contact__main__item col-12 col-lg-4">
          <div class="contact__main__item__image" style="background-color:#2E9ADB;">
            {{> ../molecules/office-item-img}}
          </div>
          <div class="contact__main__item__content">
            {{> ../molecules/office-item-content}}
          </div>
        </div>

        <div class="contact__main__item col-12 col-lg-4">
          <div class="contact__main__item__image" style="background-color:#DB7A1D; ">
            {{> ../molecules/chart-item-img}}
          </div>
          <div class="contact__main__item__content">
            {{> ../molecules/chart-item-content}}
          </div>
        </div>

      </div>
    </div>
  </div>
</section>
{{/org-contact-section}}